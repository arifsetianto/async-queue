{
    "name": "hyperf/async-queue",
    "description": "A async queue component for hyperf.",
    "license": "Apache-2.0",
    "keywords": [
        "php",
        "hyperf",
        "async queue"
    ],
    "support": {
    },
    "require": {
        "php": ">=7.2",
        "psr/container": "^1.0",
        "psr/event-dispatcher": "^0.7",
        "hyperf/contract": "dev-master",
        "hyperf/utils": "dev-master"
    },
    "require-dev": {
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0.0",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "suggest": {
        "hyperf/event": "Required to dispatch a event.",
        "hyperf/process": "Auto register a consumer process for server."
    },
    "autoload": {
        "files": [
        ],
        "psr-4": {
            "Hyperf\\AsyncQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
        },
        "hyperf": {
            "config": "Hyperf\\AsyncQueue\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
